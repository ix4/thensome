    "spencer-color": "0.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babelify": "^10.0.0",
    "budo": "^11.5.0",
    "derequire": "^2.0.6",
    "uglify-js": "^3.4.9",
    "wtf-mlb": "^0.2.0",
    "wtf_wikipedia": "^7.0.0"
  }
}
